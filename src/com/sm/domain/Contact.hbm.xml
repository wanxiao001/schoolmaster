<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sm.domain.Contact" table="tdcontact">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <property name="userId" column="userId"></property>
        <property name="uuerId" column="uuerId"></property>
        <property name="contactTime" column="contactTime"></property>
        <property name="contacter" column="contacter"></property>
        <property name="QQ" column="QQ"></property>
        <property name="message" column="message"></property>
        <property name="email" column="email"></property>
     </class>
</hibernate-mapping>