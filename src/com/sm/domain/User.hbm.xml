<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sm.domain.User" table="tduser">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <property name="userName" column="userName"></property>
        <property name="passWord" column="passWord"></property>
        <property name="age" column="age"></property>
        <property name="sex" column="sex"></property>
        <property name="creatTime" column="creatTime"></property>
        <property name="school" column="school"></property>
        <property name="QQ" column="QQ"></property>
        <property name="email" column="email"></property>
        <property name="master" column="master"></property>
        <property name="description" column="description"></property>
        <property name="skill" column="skill"></property>
        <property name="introduction" column="introduction"></property>
        <property name="star" column="star"></property>
        <property name="comments" column="comments"></property>
        <property name="admin" column="admin"></property>
     </class>
</hibernate-mapping>